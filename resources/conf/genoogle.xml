<genoogle:conf xmlns:genoogle="http://genoogle.pih.bio.br">

    <genoogle:search-manager>
        <genoogle:max-simultaneous-searchs value="2"/>
    </genoogle:search-manager>

    <genoogle:score>
        <genoogle:match value="1"/>
        <genoogle:mismatch value="-3"/>
    </genoogle:score>

    <genoogle:search-parameters>
        <genoogle:max-sub-sequence-distance value="30"/> <!-- default value="30" -->
        <genoogle:min-hsp-length value="11"/> <!-- default value="11" /> -->
        <genoogle:extend-dropoff value="5"/> <!-- default value="5" /> -->
        <genoogle:max-hits-results value="50"/> <!-- default value="50" /> -->
        <genoogle:max-threads-index-search value="2"/> <!-- default value="2" /> -->
        <genoogle:max-threads-extend-align value="2"/> <!-- default value="4" /> -->
        <genoogle:min-query-slice-length value="1000"/> <!-- default value="1000" /> -->
        <genoogle:query-split-quantity value="1"/> <!-- default value="1" /> -->
    </genoogle:search-parameters>

    <genoogle:web-service>
        <genoogle:server-address value="http://localhost:8090/webservices?wsdl"/>
        <genoogle:use-sessions value="true"/>
    </genoogle:web-service>

    <genoogle:databanks>
        <genoogle:split-databanks name="sars-cov-2-2021" path="files/fasta" mask="111010010100110111"
                                  number-of-sub-databanks="1" sub-sequence-length="11" low-complexity-filter="5">
            <genoogle:databank name="sars-cov-2-2021-01" path="sars-cov-2-2021-01.fasta"/>
            <genoogle:databank name="sars-cov-2-2021-02" path="sars-cov-2-2021-02.fasta"/>
            <genoogle:databank name="sars-cov-2-2021-03" path="sars-cov-2-2021-03.fasta"/>
            <genoogle:databank name="sars-cov-2-2021-04" path="sars-cov-2-2021-04.fasta"/>
            <genoogle:databank name="sars-cov-2-2021-05" path="sars-cov-2-2021-05.fasta"/>
            <genoogle:databank name="sars-cov-2-2021-06" path="sars-cov-2-2021-06.fasta"/>
            <genoogle:databank name="sars-cov-2-2021-07" path="sars-cov-2-2021-07.fasta"/>
            <genoogle:databank name="sars-cov-2-2021-08" path="sars-cov-2-2021-08.fasta"/>
            <genoogle:databank name="sars-cov-2-2021-09" path="sars-cov-2-2021-09.fasta"/>
            <genoogle:databank name="sars-cov-2-2021-10" path="sars-cov-2-2021-10.fasta"/>
            <genoogle:databank name="sars-cov-2-2021-11" path="sars-cov-2-2021-11.fasta"/>
            <genoogle:databank name="sars-cov-2-2021-12" path="sars-cov-2-2021-12.fasta"/>
        </genoogle:split-databanks>
    </genoogle:databanks>

</genoogle:conf>